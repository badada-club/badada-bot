// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
    output   = "./generated"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Event {
    id              Int      @default(autoincrement())
    creator_chat_id Int
    date            DateTime
    cost            Int?

    @@id([id])
    @@map("events")
}
